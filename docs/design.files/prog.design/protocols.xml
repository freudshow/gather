	<common>
			<sadd>37060001</sadd>
			<oadd>0001</oadd>
			<func_type>heart_beat</func_type>
			<oper_type></oper_type>
	</common>
</root>

<!-- 上位机回应心跳(如果有其他数据将要传输到该集中器，那么就不必下发此心跳的心跳回应) -->
